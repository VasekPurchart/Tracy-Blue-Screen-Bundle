{
	"name": "vasek-purchart/tracy-blue-screen-bundle",
	"description": "This bundle lets you use the Tracy's debug screen in combination with the the default profiler in your Symfony application.",
	"keywords": ["tracy", "bluescreen", "error", "debug", "debugger", "exceptions", "bundle"],
	"license": "MIT",
	"authors": [
		{
			"name": "Va≈°ek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		}
	],
	"require": {
		"php": "~5.6|~7.0",
		"symfony/config": "~2.6|~3.0",
		"symfony/console": "~2.6|~3.0",
		"symfony/dependency-injection": "~2.6|~3.0",
		"symfony/http-kernel": "~2.6|~3.0",
		"symfony/yaml": "~2.6|~3.0",
		"tracy/tracy": "~2.4"
	},
	"require-dev": {
		"consistence/coding-standard": "0.11.1",
		"jakub-onderka/php-console-highlighter": "0.3.2",
		"jakub-onderka/php-parallel-lint": "0.9.2",
		"matthiasnoback/symfony-dependency-injection-test": "1.0",
		"mikey179/vfsStream": "1.6.4",
		"phing/phing": "2.14",
		"phpunit/phpunit": "5.5"
	},
	"autoload": {
		"psr-4": {
			"VasekPurchart\\TracyBlueScreenBundle\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"VasekPurchart\\TracyBlueScreenBundle\\": "tests"
		}
	},
	"config": {
		"bin-dir": "bin"
	}
}
