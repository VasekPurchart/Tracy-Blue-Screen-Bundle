{
	"name": "vasek-purchart/tracy-blue-screen-bundle",
	"description": "This bundle lets you use the Tracy's debug screen in combination with the the default profiler in your Symfony application.",
	"keywords": ["tracy", "bluescreen", "error", "debug", "debugger", "exceptions", "bundle"],
	"license": "MIT",
	"authors": [
		{
			"name": "Va≈°ek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		}
	],
	"require": {
		"php": "~7.2",
		"symfony/config": "~4.0",
		"symfony/console": "~4.0",
		"symfony/dependency-injection": "~4.0",
		"symfony/http-kernel": "~4.0",
		"symfony/twig-bundle": "~4.0",
		"symfony/yaml": "~4.0",
		"tracy/tracy": "~2.4"
	},
	"require-dev": {
		"consistence/coding-standard": "3.7",
		"matthiasnoback/symfony-dependency-injection-test": "3.1",
		"mikey179/vfsstream": "1.6.5",
		"php-parallel-lint/php-console-highlighter": "1.0",
		"php-parallel-lint/php-parallel-lint": "1.3.2",
		"phing/phing": "2.16.1",
		"phpunit/phpunit": "7.5.1"
	},
	"autoload": {
		"psr-4": {
			"VasekPurchart\\TracyBlueScreenBundle\\": "src"
		},
		"classmap": [ "src" ]
	},
	"autoload-dev": {
		"psr-4": {
			"VasekPurchart\\TracyBlueScreenBundle\\": "tests"
		}
	},
	"config": {
		"bin-dir": "bin"
	}
}
